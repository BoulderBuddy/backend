{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "User",
    "type": "object",
    "properties": {
        "first_name": {
            "type": ["string","null"]
        },
        "surname": {
            "type": ["string","null"]
        },
        "email": {
            "type": "string",
            "format": "email"
        },
        "is_superuser": {
            "type": "boolean",
            "default": false
        },
        "id": {
            "type": "integer"
        }
    },
    "required": ["id", "email"]
}
